    "#input has been taken for you\n",
    "n=int(input())\n",
    "size = 2*n+1\n",
    "next_pascal = [0] * size\n",
    "\n",
    "next_pascal[n] = 1\n",
    "for i in range(2, n+1):\n",
    "    pascal = list(next_pascal)\n",
    "    for j in range(1, size-1):\n",
    "        next_pascal[j] = pascal[j-1] + pascal[j+1]\n",
    "print(next_pascal[1::2])"
   ]
